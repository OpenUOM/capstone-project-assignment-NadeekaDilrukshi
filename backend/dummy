CREATE TABLE IF NOT EXISTS teacher (
  id INTEGER ,
  name TEXT NOT NULL,
  age INTEGER NOT NULL
);

CREATE TABLE IF NOT EXISTS student (
  id INTEGER PRIMARY KEY AUTOINCREMENT,
  name TEXT NOT NULL,
  age INTEGER NOT NULL,
  hometown TEXT NOT NULL
);
